配送先編集
<%= form_with model: @address, url:"/address/#{@address.id}", local:true do |f| %>

  <div>
    郵便番号（ハイフンなし）
    <%= f.text_field :postal_code %>
  </div>
  <div>
    住所
    <%= f.text_field :address %>
  </div>
  <div>
    宛名
    <%= f.text_field :name %>
  </div>
  <%= f.submit '変更内容を保存' ,class:"btn btn-success" %>
<% end %>